<?xml version="1.0" encoding="ISO-8859-1" ?>

<xsl:stylesheet version="1.0" extension-element-prefixes="mxslt"
    xmlns:mxslt="http://www.modxslt.org/ns/1.0"
    xmlns:xsl="http://www.w3.org/1999/XSL/Transform">

  <xsl:param name="menu.vspace">10</xsl:param>
  <xsl:param name="menu.border">1</xsl:param>
  <xsl:param name="menu.arrow">0</xsl:param>

  <xsl:param name="graphic.usebars">1</xsl:param>
  
  <xsl:param name="httpRoot">http://www.modxslt.org</xsl:param>
  <xsl:param name="fileRoot">/home/http/modxslt.org/http</xsl:param>
  <xsl:param name="xsltRoot">/home/http/modxslt.org/lib/xslt</xsl:param>
  <xsl:param name="dtdRoot">/home/http/modxslt.org/lib/dtd</xsl:param>

  <xsl:param name="text.up">| Up |</xsl:param>
  <xsl:param name="text.next">| Next |</xsl:param>
  <xsl:param name="text.prev">| Previous |</xsl:param>
  <xsl:param name="text.first">| First |</xsl:param>
  <xsl:param name="text.last">| Last |</xsl:param>

  <xsl:param name="faq.index.indent">2</xsl:param>
  <xsl:param name="faq.text.indent">2</xsl:param>

  <xsl:param name="rep.max.right">10</xsl:param>
</xsl:stylesheet>
